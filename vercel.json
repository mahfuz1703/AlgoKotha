{
    "version": 2,
    "builds": [
      {
        "src": "AlgoKotha/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static",
        "config": {
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "AlgoKotha/wsgi.py"
      }
    ]
  }
